  "bugs": {
    "url": "https://github.com/kagawagao/react-admin/issues"
  },
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kagawagao/react-admin.git"
  },
  "scripts": {
