{
  "name": "react-admin",
  "version": "1.0.0",
  "description": "admin dashboard with react and antd",
  "author": "kagawa <kingsongao1221@gmail.com>",
  "homepage": "https://github.com/kagawagao/react-admin#readme",
  "bugs": {
    "url": "https://github.com/kagawagao/react-admin/issues"
  },
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kagawagao/react-admin.git"
  },
  "scripts": {
    "dev": "pixas dev",
    "build": "pixas build",
    "lint": "eslint . --ext=.js,.ts,.jsx,.tsx  --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@pixas/babel-preset-app": "^0.6.0",
    "@pixas/bundler-webpack": "^0.6.1",
    "@pixas/cli": "^0.6.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.5",
    "typescript": "^5.4.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@pixas/prettier-config",
  "stylelint": {
    "extends": "@pixas/stylelint-config",
    "rules": {
      "font-family-no-missing-generic-family-keyword": null
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
