{
  "name": "react-admin",
  "version": "1.0.0-alpha.1",
  "description": "admin - based on react and ant-design",
  "main": "index.js",
  "repository": "git@github.com:kagawagao/react-admin.git",
  "author": {
    "name": "kagawagao",
    "email": "kingsongao1221@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "lint": "eslint --max-warnings 10 .",
    "lint:fix": "npm run lint -- --fix",
    "lint:flow": "flow check",
    "lint:css": "stylelint ./**/*.less",
    "clean": "better-npm-run clean",
    "compile": "better-npm-run compile",
    "build": "npm run clean && npm run compile"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node ./server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "clean": {
      "command": "babel-node ./cli/clean",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "babel-node ./cli/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3"
  },
